let test = import("test");
let Vector = import("vector").Vector;

func vec_case_1() {
  let vec = Vector(0:10);
  assert_eq(vec.size(), 10);
}

func vec_case_2() {
  let vec = Vector(0:10);
  assert_eq(vec.size(), 8);
}

let vec_suite = test.Suite("vector");

vec_suite.add_case(test.Case("vec case 1", vec_case_1));
vec_suite.add_case(test.Case("vec case 2", vec_case_2));

vec_suite.run();
